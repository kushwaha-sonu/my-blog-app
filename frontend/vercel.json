{
  "version": 2,
  "builds": [
    { "src": "./src/*.html", "use": "@vercel/static" },
    { "src": "./src/*.js", "use": "@vercel/static" },
    { "src": "./src/*.css", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/static/js/(.*)", "dest": "/src/$1.js" },
    { "src": "/static/css/(.*)", "dest": "/src/$1.css" },
    { "src": "/(.*)", "dest": "/src/index.html" }
  ]
}