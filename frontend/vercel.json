{
  "version": 2,
  "builds": [
    {
      "src": "public/static/**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/static/**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "public/**/*.js",  
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/public/static/$1"
    },
    {
      "src": "/static/css/(.*)",
      "dest": "/public/static/output.css"
    }
  ]
}