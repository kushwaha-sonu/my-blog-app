{
  "version": 2,
  "builds": [
    {
      "src": "public/static/**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/public/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/static/$1"
    }
  ]
}
