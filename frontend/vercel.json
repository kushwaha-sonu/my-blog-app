{
  "version": 2,
  "builds": [
    {
      "src": "public/static/**/*.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/static/**/*.css",
      "use": "@vercel/static"
    },
    {
      "src": "public/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/public/(.*)",
      "dest": "/public/"
    },
    {
      "src": "/static/(.*)",
      "dest": "/public/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/public/static/$1"
    },
    {
      "src": "/(.+)/",
      "dest": "/public/static/$1/index.html"
    },
    {
      "src": "/(.+)",
      "dest": "/public/static/$1.html"
    }
  ]
}
