{
  "version": 2,
  "builds": [
    { "src": "./src/*.html", "use": "@vercel/static" },
    { "src": "./src/index.js", "use": "@vercel/static" },
    { "src": "./src/output.css", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/static/js/(.*)", "dest": "/src/index.js" },
    { "src": "/static/css/(.*)", "dest": "/src/output.css" },
    { "src": "/(.*)", "dest": "/src/index.html" }
  ]
}